# Copy this file to .env.local and fill in the secrets for local development.
# Values here are safe defaults or placeholders; they are not meant for production use.

# AWS + Serverless
AWS_REGION=us-east-1
TABLE_NAME=projects-api-dev-Projects

# Cognito configuration (replace with your pool + client id)
COGNITO_USER_POOL_ID=us-east-1_example
COGNITO_USER_POOL_CLIENT_ID=exampleclientid123

# Optional local overrides
IS_OFFLINE=true
DYNAMODB_ENDPOINT=http://localhost:8000

# CORS configuration for local UI development
CORS_ALLOWED_ORIGIN=http://localhost:5173
CORS_ALLOWED_HEADERS=Content-Type,Authorization
CORS_ALLOWED_METHODS=OPTIONS,GET,POST,PUT,DELETE
